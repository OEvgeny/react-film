{
  "name": "bundle",
  "version": "0.0.0-0",
  "main": "lib/index.js",
  "scripts": {
    "build": "npm run build:babel && npm run build:webpack",
    "build:babel": "babel --out-dir lib --source-maps inline --verbose src/",
    "build:webpack": "webpack --config webpack-instrumented.config.js",
    "eslint": "eslint src/*.js src/**/*.js --ignore-pattern *.spec.js --ignore-pattern *.test.js",
    "prepublishOnly": "npm run prepublishOnly:babel && npm run prepublishOnly:webpack",
    "prepublishOnly:babel": "npm run build:babel",
    "prepublishOnly:webpack": "webpack",
    "start": "npm run build && concurrently --kill-others --names \"babel,webpack\" \"npm run start:babel\" \"npm run start:webpack\"",
    "start:babel": "npm run build:babel -- --watch",
    "start:webpack": "webpack --config webpack-watch.config.js --watch"
  },
  "dependencies": {
    "@babel/runtime": "7.11.2",
    "html-react-parser": "0.13.0",
    "react-film": "0.0.0-0"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.11.0",
    "@babel/plugin-transform-runtime": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "babel-eslint": "^10.1.0",
    "concurrently": "^5.3.0",
    "eslint": "^7.7.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.0",
    "isomorphic-react": "0.0.0-0",
    "isomorphic-react-dom": "0.0.0-0",
    "prettier": "^2.1.0",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-stats-plugin": "^0.3.2"
  }
}
